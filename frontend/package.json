{
  "name": "frontend",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start:dev": "ts-node-dev --respawn --transpile-only --project tsconfig.server.json src/main.ts",
    "start": "node dist/main.js",
    "build:server": "tsc -p tsconfig.server.json",
    "client:dev": "vite --config client/vite.config.ts",
    "client:build": "vite build --config client/vite.config.ts",
    "client:preview": "vite preview --config client/vite.config.ts",
    "test": "cd client && vitest run",
    "test:watch": "cd client && vitest",
    "build": "npm run client:build && npm run build:server"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/serve-static": "^5.0.0",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.555.0",
    "plotly.js": "^3.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-plotly.js": "^2.6.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@types/node": "^20.12.14",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/react-plotly.js": "^2.6.3",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "@testing-library/react": "^16.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "jsdom": "^25.0.0",
    "vitest": "^2.1.3"
  }
}
